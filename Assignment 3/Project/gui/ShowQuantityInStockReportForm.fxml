<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<Pane fx:id="MyPane" prefHeight="800.0" prefWidth="1050.0" stylesheets="@../styleCSS/HomePage.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.ShowQuantityInStockReportControllerClient">
   <children>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1050.0">
         <children>
            <Button fx:id="btnBack" layoutX="27.0" layoutY="750.0" mnemonicParsing="false" onMouseClicked="#backFunction" prefHeight="30.0" prefWidth="160.0" style="-fx-border-radius: 3px; -fx-border-width: 0.7; -fx-border-color: BLACK;" text="Back">
               <effect>
                  <InnerShadow blurType="ONE_PASS_BOX" color="#7789999a" height="255.0" radius="127.0" width="255.0">
                     <input>
                        <InnerShadow choke="0.57" color="#ff000000" />
                     </input>
                  </InnerShadow>
               </effect>
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="75.0" prefWidth="1050.0" style="-fx-background-color: LIGHTSLATEGRAY;">
               <effect>
                  <DropShadow blurType="ONE_PASS_BOX" color="#faf9f9" height="10.0" radius="4.5" width="10.0" />
               </effect>
               <children>
                  <Label layoutX="14.0" layoutY="21.0" text="System Operations -&gt; Show Reports -&gt; Quantity of Items In Stock" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <VBox alignment="CENTER" layoutX="1.0" layoutY="76.0" prefHeight="75.0" prefWidth="1050.0">
               <children>
                  <Label text="Quantity of Items In Stock">
                     <font>
                        <Font size="35.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Pane layoutY="149.0" prefHeight="591.0" prefWidth="1050.0">
               <children>
                  <TableView fx:id="tableQuantity" layoutX="75.0" layoutY="215.0" prefHeight="351.0" prefWidth="900.0">
                    <columns>
                      <TableColumn fx:id="clmnStationName" prefWidth="184.79991912841797" text="Station Name" />
                      <TableColumn fx:id="clmnFuelType" prefWidth="216.79986572265625" text="Fuel Type" />
                        <TableColumn fx:id="clmnQuantity" prefWidth="244.80007934570312" text="Quantity In Stock" />
                        <TableColumn fx:id="clmnQuarter" prefWidth="105.5999755859375" text="Quarter" />
                        <TableColumn fx:id="clmnYear" prefWidth="147.199951171875" text="Year" />
                    </columns>
                  </TableView>
                  <Label layoutX="43.0" layoutY="27.0" text="Your Chain:">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtChainName" layoutX="182.0" layoutY="27.0" prefHeight="25.0" prefWidth="190.0" />
                  <Label layoutX="43.0" layoutY="91.0" text="Station Name:">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="cmbStationName" layoutX="182.0" layoutY="91.0" onAction="#cmbClickStation" prefHeight="20.0" prefWidth="190.0" promptText="choose one" style="-fx-border-radius: 3px; -fx-border-image-width: 0.7; -fx-border-color: BLACK;">
                     <effect>
                        <InnerShadow blurType="ONE_PASS_BOX" color="#77899999" height="255.0" radius="127.0" width="255.0">
                           <input>
                              <InnerShadow choke="0.57" color="#ff000000" />
                           </input>
                        </InnerShadow>
                     </effect>
                  </ComboBox>
                  <Label layoutX="43.0" layoutY="155.0" text="Year:">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="cmbxYear" layoutX="182.0" layoutY="155.0" onAction="#clickedYearCmbx" prefHeight="20.0" prefWidth="190.0" promptText="choose one" style="-fx-border-radius: 3px; -fx-border-image-width: 0.7; -fx-border-color: BLACK;">
                     <effect>
                        <InnerShadow blurType="ONE_PASS_BOX" color="#77899999" height="255.0" radius="127.0" width="255.0">
                           <input>
                              <InnerShadow choke="0.57" color="#ff000000" />
                           </input>
                        </InnerShadow>
                     </effect>
                  </ComboBox>
               </children>
            </Pane>
            <HBox layoutX="985.0" layoutY="5.0" prefHeight="24.0" prefWidth="27.0">
               <children>
                  <Button fx:id="Minus" mnemonicParsing="false" onAction="#minimize" styleClass="minimizeExitBtn">
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../Images/Minus.png" />
                           </image>
                           <effect>
                              <ColorAdjust brightness="1.0" contrast="-1.0" />
                           </effect>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button fx:id="X" mnemonicParsing="false" onAction="#exit" styleClass="minimizeExitBtn">
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../Images/X.png" />
                           </image>
                           <effect>
                              <ColorAdjust brightness="1.0" contrast="1.0" />
                           </effect>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</Pane>
